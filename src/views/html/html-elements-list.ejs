<div class="html-element-list">

    <% for (const html of data) { %>

        <div class="html-element">
            <h3 class="element-title"><%= html.description %></h3>
            <code class="html-content">
                <%= html.html %>
            </code>
            <div class="button-wrapper">
                <a href="edit/<%= html._id %>" class="btn edit">Edytuj</a>
                <button class="btn remove" data-element="<%= html._id %>">Usu≈Ñ</button>
            </div>
        </div>

    <% } %>

</div>